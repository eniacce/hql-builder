<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.tools.hql-builder.demo</groupId>
		<artifactId>hql-builder-demo</artifactId>
		<version>2.0.1-SNAPSHOT</version>
		<relativePath>../pom.xml</relativePath>
	</parent>
	<artifactId>hql-builder-common-test</artifactId>
	<name>hql-builder-common-test</name>
	<description>hql-builder-common-test</description>
	<dependencies>
		<dependency>
			<groupId>org.tools.hql-builder.hibernate</groupId>
			<artifactId>hibernate-${version.hibernate}</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>org.tools.hql-builder</groupId>
			<artifactId>hql-builder-common</artifactId>
			<version>${project.version}</version>
			<exclusions>
				<exclusion>
					<groupId>org.tools.hql-builder.hibernate</groupId>
					<artifactId>hibernate-3.2</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.tools.hql-builder.hibernate</groupId>
					<artifactId>hibernate-3.3</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.tools.hql-builder.hibernate</groupId>
					<artifactId>hibernate-3.5</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.tools.hql-builder.hibernate</groupId>
					<artifactId>hibernate-3.6</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.tools.hql-builder.hibernate</groupId>
					<artifactId>hibernate-4.0</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.tools.hql-builder.hibernate</groupId>
					<artifactId>hibernate-4.1</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.tools.hql-builder.hibernate</groupId>
					<artifactId>hibernate-4.2</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-web</artifactId>
			<version>${version.spring}</version>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-aop</artifactId>
			<version>${version.spring}</version>
		</dependency>
		<dependency>
			<groupId>joda-time</groupId>
			<artifactId>joda-time</artifactId>
			<version>${version.joda}</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-deploy-plugin</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>