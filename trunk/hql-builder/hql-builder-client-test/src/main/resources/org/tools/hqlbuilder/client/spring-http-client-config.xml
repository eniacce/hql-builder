	<import resource="classpath:org/tools/hqlbuilder/client/applicationContext.xml" />

	<bean id="propertyPlaceholderConfigurer" class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer" scope="singleton" autowire-candidate="false">
		<property name="locations">
			<list>
				<value>classpath:org/tools/hqlbuilder/client/hqlwebclient.properties</value>
			</list>
		</property>
	</bean>

</bean>