	-webkit-appearance: none !important;
	border-radius: 20px;
	background-color: @form_element_background;
	box-shadow: inset 0 0 10px rgba(000, 000, 000, 0.5);
	border: 1px solid #999;
	height: 20px;
	width: 20px;
}

