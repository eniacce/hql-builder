	-webkit-appearance: none;
	border-radius: 5px;
	box-shadow: 0 1px 1px @form_element_border inset, 0 1px 0 @form_element_border_gradient;
	background-color: @form_button_background;
	height: 10px;
	vertical-align: middle;
}

