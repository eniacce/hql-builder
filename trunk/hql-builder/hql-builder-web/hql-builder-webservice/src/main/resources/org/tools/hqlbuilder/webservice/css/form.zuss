/* validation etc */

.requiredMarker {
	background-image: url("images/bullet_star.png");
	background-position: top;
	background-repeat: no-repeat;
	display: inline;
	height: 16px;
	width: 16px;
	color: transparent;
	text-align: center;
	vertical-align: middle;
	padding-left: 3px;
	padding-right: 3px;
}

.requiredMakeup {
	font-size: small;
	color: @form_required;
	vertical-align: top;
	padding-left: 3px;
	padding-right: 3px;
}

.feedbackPanelERROR {
	font-size: small;
	color: @form_error;
	vertical-align: top;
	padding-left: 3px;
	padding-right: 3px;
}

.feedbackPanel {
	margin: 0px 0px 0px 0px;
	padding: 0px 0px 0px 15px;
	display: inline-block;
}

.valid {
	border-color: @form_valid_border !important;
}

.invalid {
	border-color: @form_invalid_border !important;
}

.required {
	border-color: @form_required_border !important;
}

.required.valid {
	border-color: @form_valid_border !important;
}

.required.invalid {
	border-color: @form_invalid_border !important;
}

